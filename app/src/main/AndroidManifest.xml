<manifest package="com.zero.bezier"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".demo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN">
                </action>
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>
        <activity android:name=".demo.WaveActivity"></activity>
        <activity android:name=".demo.ElasticLoaderActivity"></activity>
        <activity android:name=".demo.AdhesionLayoutActivity"></activity>
        <activity android:name=".demo.AdhesionLoaderActivity"></activity>
        <activity android:name=".demo.DemoActivity"></activity>
    </application>

</manifest>
